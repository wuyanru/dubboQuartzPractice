<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" /><!-- 开启二级缓存 -->
		<setting name="lazyLoadingEnabled" value="true" /><!-- 开启懒加载 -->
		<setting name="multipleResultSetsEnabled" value="true" /><!-- 是否允许单一语句返回多结果集 -->
		<setting name="useColumnLabel" value="true" /><!-- 使用列标签代替列名。 -->
		<setting name="defaultExecutorType" value="REUSE" /><!-- 配置默认的执行器。SIMPLE 就是普通的执行器；REUSE 执行器会重用预处理语句（prepared statements）； BATCH 执行器将重用语句并执行批量更新。 -->
		<setting name="defaultStatementTimeout" value="25000" /><!-- 设置超时时间 -->
	</settings>
</configuration>